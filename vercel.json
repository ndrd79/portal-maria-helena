{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ],
  "env": {
    "NEXT_PUBLIC_FORCE_DYNAMIC": "true"
  }
}
